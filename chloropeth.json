.monaco-editor.hc-black .monaco-menu .monaco-action-bar.vertical .action-menu-item:focus .action-label {
	color: #ACDDFF;
	stroke-width: 1.2px;
	text-shadow: 0px 0px 0.15px #ACDDFF;
